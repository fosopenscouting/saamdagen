<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@id/navigation_home">

    <fragment android:id="@+id/navigation_info" android:name="be.fos.saamdagen.ui.info.InfoFragment" android:label="fragment_info"
              tools:layout="@layout/fragment_info"/>
    <fragment android:id="@+id/navigation_schedule" android:name="be.fos.saamdagen.ui.schedule.ScheduleFragment"
              android:label="fragment_schedule" tools:layout="@layout/fragment_schedule"/>
    <fragment android:id="@+id/navigation_map" android:name="be.fos.saamdagen.ui.map.MapFragment" android:label="fragment_map"
              tools:layout="@layout/fragment_map"/>
    <fragment
            android:id="@+id/navigation_home"
            android:name="be.fos.saamdagen.ui.home.HomeFragment"
            android:label="home_fragment"
            tools:layout="@layout/fragment_home">
        <action android:id="@+id/action_news_schedule"
                app:destination="@id/navigation_schedule" app:enterAnim="@anim/nav_default_enter_anim"
                app:exitAnim="@anim/nav_default_exit_anim" app:popEnterAnim="@anim/nav_default_pop_enter_anim"
                app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment android:id="@+id/navigation_activity" android:name="be.fos.saamdagen.ui.session.SessionFragment"
              android:label="ActivityFragment"/>
</navigation>